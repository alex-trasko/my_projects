{% if content.page_top %}
  <div class="white-line gray-border">
    <div class="indent">
      {{ content.page_top }}
    </div>
  </div>
{% endif %}
{% if content.catalog_head %}
<div class="white-line red-border">
    <div class="indent">
        <div class="catalog-head">
            {{ content.catalog_head }}
        </div>
    </div>
</div>
{% endif %}
<div class="gray-line">
    <div class="indent">
        <div class="container">
            <div class="breadcrumb">
                {{ content.breadcrumb }}
            </div>
            <aside class="sidebar-left" role="complementary">
                {{ content.sidebar_left }}
            </aside>
            <div class="sidebar-right white-bg">
                {{ content.sidebar_right }}
            </div>
        </div>
    </div>
</div>
{% if content.page_bottom %}
  <div class="white-line">
    <div class="indent">
      <div class="container">
        {{ content.page_bottom }}
      </div>
    </div>
  </div>
{% endif %}            
